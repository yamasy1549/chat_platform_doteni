{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "uwsgi",
      "image": "yamasy1549/chat_platform_doteni_uwsgi",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 3031,
          "containerPort": 3031
        }
      ]
    },
    {
      "name": "nginx",
      "image": "yamasy1549/chat_platform_doteni_nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "uwsgi"
      ]
    }
  ]
}
